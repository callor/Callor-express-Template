  "repository": {
    "type": "git",
    "url": "https://github.com/callor/Node_Express_Ex6_Template.git"
  },
  "homepage": "https://www.callor.com",
  "license": "(MIT OR Apache-2.0)",
  "dependencies": {
    "sorted-object": "2.0.1",
